<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

 <mapper namespace="com.example.LearningManagementSystem.User.UserDAO">
 
 	<select id="userList" resultType="userdto">
 		select * from user where User_Status not in(0,2);
 	</select>
 	
 	<select id="oneUser" parameterType="String" resultType="userdto">
 		select * from user where user_Id=#{user_Id}
 	</select>
 </mapper>